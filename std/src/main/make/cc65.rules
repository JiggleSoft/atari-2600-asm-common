#==============================================================================
# Makefile CC65 Rules
#==============================================================================

# CC65 application name variables.

export AR65     := ar65
export CA65     := ca65
export CC65     := cc65
export CHRCVT65 := chrcvt65
export CL65     := cl65
export CO65     := co65
export DA65     := da65
export GRC65    := grc65
export LD65     := ld65
export OD65     := od65
export SIM65    := sim65
export SP65     := sp65


# CC65 override of variables used by implicit rules.

export CC  := $(CC65)
export CPP := $(CC) -E
export LD  := ld65
export AS  := ca65
export AR  := ar65


# CC65 generic application variables.

export CL := cl65
export SIM := sim65


